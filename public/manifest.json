{
  "name": "quick-google-search",
  "description": "google search support",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "images/image16.png",
    "32": "images/image32.png",
    "48": "images/image48.png",
    "128": "images/image128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search?*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "permissions": []
}
